@{
    ViewData["Title"] = "Pin Generator";
}

<div class="text-center">
    <h1 class="display-4">Pin Generator</h1>
    <p>Request a new pin to be generated from 0 to 9999</p>
</div>

<div id="PinGenerator" class="input-group mb-3">
    <div class="input-group-prepend">
        <button class="btn btn-outline-secondary new-pin" type="button">Generate Pin</button>
    </div>
    <input type="text" class="form-control generated-pin" placeholder="" aria-label="" aria-describedby="basic-addon1">
</div>